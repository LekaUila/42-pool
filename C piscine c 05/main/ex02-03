//ex02

#include <stdio.h>

int	main(void)
{
	int i;
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_iterative_power(0, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_iterative_power(1, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_iterative_power(5, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_iterative_power(-1, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_iterative_power(6, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_iterative_power(2, i));
	for (i = -5; i < 10;i++)
		printf("%i\n", ft_iterative_power(55, i));
	return (0);
}

//ex03

#include <stdio.h>

int	main(void)
{
	int i;
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_recursive_power(0, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_recursive_power(1, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_recursive_power(5, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_recursive_power(-1, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_recursive_power(6, i));
	for (i = 0; i < 10;i++)
		printf("%i\n", ft_recursive_power(2, i));
	for (i = -5; i < 10;i++)
		printf("%i\n", ft_recursive_power(55, i));
	return (0);
}
